<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.shape.Rectangle?>

<?import javafx.scene.layout.AnchorPane?>
<VBox onKeyPressed="#handleOnKeyPressed" onKeyReleased="#handleOnKeyReleased" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.SecondaryController">
    <Label alignment="CENTER" fx:id="label1" text="Secondary View"/>
    <Button fx:id="menuButton" text="Exit to menu" onAction="#switchToPrimary"/>
    <AnchorPane>
        <Rectangle fx:id="rectangle" x="10" y="20" width="100" height="50" fill="burlywood" stroke="black"/>
    </AnchorPane>
    <padding>
        <Insets top="10" bottom="10" left="10" right="10"/>
    </padding>
</VBox>
